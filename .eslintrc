{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "space-in-parens": ["error", "never"],
        "no-multiple-empty-lines": "error",
        "semi": ["error", "always"]
    }
  }